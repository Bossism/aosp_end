<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.aosp_end.dao.ClassInfoDao">
    <select id="getAll" resultType="com.example.aosp_end.bean.Classinfo">
        SELECT * FROM classinfo
    </select>

    <select id="getAllByVersionId" resultType="com.example.aosp_end.bean.Classinfo">
        SELECT * FROM classinfo where version_id = #{id}
    </select>

</mapper>